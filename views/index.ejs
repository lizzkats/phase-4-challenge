<!DOCTYPE html>
<html>
<head>
  <title>Vinyl</title>
</head>
<body>
<% include header %>
  <h2>Records</h2>

  <div class="album-list">
    <% albums.forEach(function(album) { %>
      <div>
        <h5><a href="/albums/<%= album.id %>"><%= album.title %></a></h5>
        <span><%= album.artist %></span>
      </div>
    <% }) %>
  </div>
<script type="text/javascript" src="/script.js"></script>
</body>
</html>
